var testTaskDictionary: [String: String] = [:]
print (testTaskDictionary)

func testTask(clientBonuses: Dictionary <Int, Double>) -> [Int: Int] {

    let clientId: Int = clientBonuses[12]
    let moneyUnits: Int = clientBonuses[13]
    return (clientId, moneyUnits)
}

