import UIKit


var zooArr = [String]()

func animals (animalOne:String, animalTwo:String, animalThree:String) {
    zooArr.append (contentsOf:[animalOne,animalTwo,animalThree])
}
func animalsDead (animalOne:String, animalTwo:String,animalThree:String){
    zooArr.remove(at:0)
}
animals (animalOne: "Mouse", animalTwo: "Elephant", animalThree: "Dog")
print(zooArr)

animalsDead (animalOne: "Mouse", animalTwo: "Elephant", animalThree: "Dog")
print(zooArr)



